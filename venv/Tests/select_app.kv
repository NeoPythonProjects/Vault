ScreenManager
    padding: 10,10
    spacing: 10,10
    Screen
        id: sc1
        name: 's1'
        BoxLayout
            Label:
                id: output_label
                name: "output_label"
                text: self.name
            Button
                text: 'go to screen 2'
                on_press: root.current = 's2'
            ListView

    Screen
        name: 's2'
        BoxLayout
            Button
                text: 'go to screen 1'
                on_press: root.current = 's1'
            ListView



<MyButton>:
    on_press: self.push_data(self.text, root.current)

<ListView>:
    data: [ {'text': "application:  {}       user:  {}".format(el[0], el[1])} for el in self.get_data()]
    #data: [{'text':'123'},{'text':'fdsdfs'}]
    viewclass: "MyButton"
    SelectableRecycleBoxLayout:
        default_size: None, dp(56)
        default_size_hint: 0.9, None
        size_hint_y: None
        height: self.minimum_height
        orientation: 'vertical'
        multiselect: False
        touch_multiselect: False


